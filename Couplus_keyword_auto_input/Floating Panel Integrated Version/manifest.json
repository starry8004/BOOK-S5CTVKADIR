{
    "manifest_version": 3,
    "name": "COUPLUS Keyword Search Assistant (Floating Panel)",
    "version": "1.1",
    "description": "COUPLUS 키워드 검색 자동화 도구 - 플로팅 패널 버전",
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
    "host_permissions": [
      "https://www.couplus.co.kr/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.couplus.co.kr/keyword*"],
        "js": ["selectors.js", "content.js"]
      }
    ]
  }
  